{
  "name": "@tiny-i18n/react-live",
  "version": "1.0.2",
  "main": "lib/index.js",
  "scripts": {
    "example": "run-p -l server gojs",
    "server": "cd examples && node server.js",
    "gojs": "gojs -i examples/spa.js -p 10000",
    "build": "gojs -b examples/index.js",
    "build:css": "lessc lib/style.less lib/style.css",
    "prepare": "npm run build:css && babel src/ -D -d lib",
    "dev": "npm run prepare -- -w"
  },
  "description": "The magical effect making tiny-i18n could be used easily and edit live in react.",
  "keywords": [
    "tiny-i18n",
    "react",
    "edit-live",
    "live",
    "internationalization",
    "i18n"
  ],
  "devDependencies": {
    "@tiny-i18n/express-live": "^1.0.2",
    "express": "^4.16.3",
    "go-js": "^1.3.12",
    "less": "^3.0.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "tiny-i18n": "^1.0.2"
  },
  "peerDependencies": {
    "react": "*",
    "tiny-i18n": "^1.0.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "prop-types": "^15.6.1"
  },
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "author": "imcuttle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imcuttle/tiny-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/imcuttle/tiny-i18n/issues"
  },
  "homepage": "https://github.com/imcuttle/tiny-i18n#readme",
  "publishConfig": {
    "access": "public"
  }
}
