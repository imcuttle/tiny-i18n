{
  "name": "tiny-i18n",
  "version": "0.0.0",
  "description": "tiny i18n",
  "main": "lib/tiny-i18n/index.js",
  "scripts": {
    "test": "jest",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "babel": "rimraf lib && babel src/ -d lib/ -D",
    "rollup": "rimraf dist && rollup -c",
    "prepare": "run-p rollup babel",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "keywords": [
    "imcuttle",
    "tiny-i18n"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "imcuttle",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-eslint": "^8.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "conventional-changelog-cli": "^1.3.16",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.18.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lerna": "^2.7.1",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imcuttle/tiny-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/imcuttle/tiny-i18n/issues"
  },
  "homepage": "https://github.com/imcuttle/tiny-i18n#readme"
}
